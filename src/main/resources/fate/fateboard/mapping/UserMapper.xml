<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webank.ai.fate.board.dao.UserMapper">


    <select id="find" resultType="com.webank.ai.fate.board.pojo.UserDO">

        SELECT * FROM t_fateboard_user
        where name = #{name}
        and password = #{password}

    </select>
</mapper>